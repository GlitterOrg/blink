interface LayoutNode {
    DOMString getCSSValue(DOMString value);

    readonly attribute LayoutParent parent;
    readonly attribute sequence<LayoutChild> children;

    readonly attribute double width; // Should be inline size, should only be availible in calculateHeight phase.
    readonly attribute double height; // Should be box size, should only be availible in calculateHeight phase.

    readonly attribute double overrideWidth;

    void log(DOMString str);

    void setWidth(double width);
    [CallWith=ScriptState] void setHeight(Promise height);
};
